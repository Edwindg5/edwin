<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AgregarGanadoController">
    <children>
        <Label layoutX="30.0" layoutY="30.0" text="Nombre:" />
        <TextField fx:id="txtNombre" layoutX="100.0" layoutY="30.0" />
        <Label layoutX="30.0" layoutY="70.0" text="ID:" />
        <TextField fx:id="txtId" layoutX="100.0" layoutY="70.0" />
        <Label layoutX="30.0" layoutY="110.0" text="Peso:" />
        <TextField fx:id="txtPeso" layoutX="100.0" layoutY="110.0" />
        <Label layoutX="30.0" layoutY="150.0" text="Estado de Salud:" />
        <TextField fx:id="txtEstadoSalud" layoutX="150.0" layoutY="150.0" />
        <Label layoutX="30.0" layoutY="190.0" text="Raza:" />
        <TextField fx:id="txtRaza" layoutX="100.0" layoutY="190.0" />
        <Button layoutX="30.0" layoutY="230.0" mnemonicParsing="false" onAction="#agregarGanado" text="Agregar" />
    </children>
</AnchorPane>

