<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.proyectomulti.controller.LoginController">
    <children>
        <Label layoutX="30.0" layoutY="30.0" text="Usuario:" />
        <TextField fx:id="usuarioField" layoutX="100.0" layoutY="30.0" />
        <Label layoutX="30.0" layoutY="70.0" text="Contraseña:" />
        <PasswordField fx:id="contrasenaField" layoutX="100.0" layoutY="70.0" />
        <Button layoutX="30.0" layoutY="110.0" mnemonicParsing="false" onAction="#login" text="Iniciar Sesión" />
    </children>
</AnchorPane>
